# Velora Frontend - Environment Variables Template
# Copy this file to .env.local and fill in your actual API keys

# =============================================================================
# REQUIRED: Backend API (port must match where Flask runs: 5000 or 5001 if 5000 in use)
# =============================================================================
VITE_BACKEND_URL=http://localhost:5001

# =============================================================================
# REQUIRED: Mapbox for Maps
# =============================================================================
VITE_MAPBOX_TOKEN=pk.your_mapbox_token_here

# =============================================================================
# OPTIONAL: AI Services (if not using backend proxy yet)
# =============================================================================
VITE_OPENAI_API_KEY=sk-your_openai_key_here
VITE_GOOGLE_CLOUD_VISION_API_KEY=your_google_vision_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here
VITE_COHERE_API_KEY=your_cohere_key_here
